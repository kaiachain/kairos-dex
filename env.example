# Uniswap V3 DEX - Environment Variables Example
# Copy this file to .env.local and update values as needed
# All variables below are required unless marked as optional

# =============================================================================
# Network Configuration
# =============================================================================
VITE_CHAIN_ID=1001
VITE_CHAIN_NAME=Kairos Testnet
VITE_CHAIN_NETWORK=kairos-testnet
VITE_NATIVE_CURRENCY_NAME=KAIA
VITE_NATIVE_CURRENCY_SYMBOL=KAIA
VITE_NATIVE_CURRENCY_DECIMALS=18

# =============================================================================
# RPC URLs
# =============================================================================
VITE_RPC_URL=https://public-en-kairos.node.kaia.io

# =============================================================================
# Block Explorer
# =============================================================================
VITE_BLOCK_EXPLORER_NAME=Kairos Explorer
VITE_BLOCK_EXPLORER_URL=https://kairos.kaiascan.io

# =============================================================================
# Contract Addresses
# =============================================================================
VITE_V3_CORE_FACTORY=0xb522cF1A5579c0EAe37Da6797aeBcE1bac2D4a29
VITE_SWAP_ROUTER_02=0xd28909Ef8bd258DCeFD8B5A380ff55f92eD8ae4b
VITE_NONFUNGIBLE_POSITION_MANAGER=0x9546E23b2642334E7B82027B09e5c6c8E808F4E3
VITE_QUOTER_V2=0x56a4BD4a66785Af030A2003254E93f111892BfB5
VITE_MULTICALL2=0x2A2aDD27F8C70f6161C9F29ea06D4e171E55C680
VITE_TICK_LENS=0x56C8DAB2fFf78D49a76B897828E7c58896bA8b87
VITE_V3_MIGRATOR=0xDab424Aba37f24A94f568Df345634d4B66830ebB
VITE_V3_STAKER=0xc3cF7B37E5020f718aceE1f4e1b12bC7b1C6CE4B
VITE_WRAPPED_NATIVE_TOKEN=0x043c471bee060e00a56ccd02c0ca286808a5a436

# =============================================================================
# WalletConnect (Optional)
# =============================================================================
# Get your project ID from https://cloud.walletconnect.com
# Leave empty if not using WalletConnect
VITE_WALLETCONNECT_PROJECT_ID=

# =============================================================================
# The Graph Subgraph
# =============================================================================
# For The Graph Studio (development/testing - no auth required):
# VITE_SUBGRAPH_URL=https://api.studio.thegraph.com/query/[project-id]/[subgraph-name]/version/latest

# For The Graph Network (production - requires API key):
# VITE_SUBGRAPH_URL=https://gateway.thegraph.com/api/[api-key]/subgraphs/id/[subgraph-id]
# OR (hosted service):
# VITE_SUBGRAPH_URL=https://api.thegraph.com/subgraphs/name/[username]/[subgraph-name]

VITE_SUBGRAPH_URL=https://api.studio.thegraph.com/query/102730/kairos-dex/version/latest

# Bearer token for authenticated requests (required for The Graph Network)
# Optional for Studio, but recommended for production
# Get your API key from:
# - Studio: https://thegraph.com/studio/apikeys/
# - Network: https://thegraph.com/explorer/
VITE_SUBGRAPH_BEARER_TOKEN=your_subgraph_bearer_token

# =============================================================================
# App Configuration
# =============================================================================
VITE_APP_NAME=Kairos DEX
VITE_IS_TESTNET=true
